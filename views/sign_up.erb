<form id="sign_up_form" action="/sign_up" method="post">
  <h2 class="page_title">Sign Up</h2>
  <p>
    <input name="email" type="text" class="textbox" placeholder="Email Address"/>
  </p>
  <p>
    <input name="name" type="text" class="textbox" placeholder="Full Name" />
  </p>
  <p>
    <input name="phone" type="text" class="textbox" placeholder="Phone Number"/>
  </p>
  <p>
    <input name="password" type="password" class="textbox" placeholder="Password"/>
  </p>
  <p>
    <input class="app-button" type="submit" value="Submit" />
  </p>
  <p class="top-20">
    All fields are required.
  </p>
  <p>
    Already have an account? <a href="/login">Login</a>
  </p>
</form>




